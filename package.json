{
  "name": "hacktj-2021",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org/2021",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "^10.1.0",
    "chart.js": "^3.0.0-beta.10",
    "eslint": "^7.19.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-map-gl": "^6.1.4",
    "react-parallax-tilt": "^1.4.76",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.2",
    "reactstrap": "^8.9.0",
    "web-vitals": "^1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn run switch-homepage && react-scripts start",
    "build": "react-scripts build && yarn run postbuild",
    "postbuild": "react-snap; purgecss --content build/**/*.html --css build/static/{css/*.css,js/*.js} --output build/static/css --font-face --keyframes --rejected",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier . --write; eslint src/ --fix",
    "tint": "./tint-sponsors '#269999'",
    "switch-event": "./switch event",
    "switch-homepage": "./switch homepage",
    "predeploy": "yarn run build && NODE_DEBUG=gh-pages",
    "deploy-event": "yarn run switch-event && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -d build --dotfiles -m \"Update $timestamp\"",
    "deploy-homepage": "yarn run switch-homepage && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -b main -d build --dotfiles -m \"Update $timestamp\" && yarn run switch-event",
    "deploy-all": "yarn run deploy-event && yarn run deploy-homepage"
  },
  "reactSnap": {
    "minifyCss": {
      "inline": [
        "all"
      ],
      "level": 2,
      "sourceMap": true,
      "sourceMapInlineSources": true
    },
    "fixWebpackChunksIssue": false,
    "inlineCss": false,
    "cacheAjaxRequests": true,
    "http2PushManifest": true,
    "preloadImages": true,
    "asyncScriptTags": true
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@types/chart.js": "^2.9.30",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/preval.macro": "^3.0.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-map-gl": "^5.2.9",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "htmlparser2": "^6.0.0",
    "jq.node": "^2.2.0",
    "prettier": "^2.2.1",
    "preval.macro": "^5.0.0",
    "purgecss": "^4.0.0",
    "react-refresh": "^0.9.0",
    "react-snap": "^1.23.0",
    "serve": "^11.3.2",
    "sharp-cli": "^1.14.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HackTJ/2021.git"
  },
  "bugs": {
    "url": "https://github.com/HackTJ/2021/issues"
  }
}
