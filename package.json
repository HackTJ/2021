{
  "name": "hacktj-2021",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org/2021",
  "dependencies": {
    "chart.js": "^3.0.0-beta.6",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.12.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-map-gl": "^5.2.10",
    "react-parallax-tilt": "^1.4.66",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.0",
    "react-spinners": "^0.9.0",
    "reactstrap": "^8.7.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn run switch-homepage && react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap; purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier src/ --write; eslint src/ --fix",
    "tint": "./tint-sponsors '#269999'",
    "switch-event": "./switch event",
    "switch-homepage": "./switch homepage",
    "predeploy": "yarn run build && NODE_DEBUG=gh-pages",
    "deploy-event": "yarn run switch-event && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -d build -m \"Update $timestamp\"",
    "deploy-homepage": "yarn run switch-homepage && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -b master -d build -m \"Update $timestamp\" && yarn run switch-event",
    "deploy-all": "yarn run deploy-event && yarn run deploy-homepage"
  },
  "reactSnap": {
    "http2PushManifest": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks",
      "airbnb-typescript-prettier",
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended",
      "prettier/react",
      "prettier/standard",
      "plugin:jsx-a11y/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "rules": {
      "global-require": 0,
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@types/chart.js": "^2.9.28",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.8",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-map-gl": "^5.2.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript-prettier": "^4.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "jq.node": "^2.2.0",
    "prettier": "^2.1.2",
    "purgecss": "^3.0.0",
    "react-refresh": "^0.9.0",
    "react-snap": "^1.23.0",
    "serve": "^11.3.2",
    "sharp-cli": "^1.14.1",
    "source-map-explorer": "^2.5.0",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HackTJ/2021.git"
  },
  "bugs": {
    "url": "https://github.com/HackTJ/2021/issues"
  }
}
