{
  "name": "hacktj-2021",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org/2021",
  "dependencies": {
    "@types/chart.js": "^2.9.22",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-map-gl": "^5.2.4",
    "@types/reactstrap": "^8.4.3",
    "@typescript-eslint/parser": "^3.0.0",
    "chart.js": "^3.0.0-alpha",
    "history": "^5.0.0",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.11.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-map-gl": "^5.2.7",
    "react-parallax-tilt": "^1.4.57",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.1",
    "react-spinners": "^0.9.0",
    "reactstrap": "^8.4.1",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.6",
    "web-vitals": "^0.2.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn run switch-homepage && react-scripts start",
    "build": "react-scripts build",
    "postbuild": "purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier src/ --write; eslint src/ --fix",
    "tint": "./tint-sponsors '#269999'",
    "switch-event": "./switch event",
    "switch-homepage": "./switch homepage",
    "predeploy": "yarn run build && NODE_DEBUG=gh-pages",
    "deploy-event": "yarn run switch-event && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -d build -m \"Update $timestamp\"",
    "deploy-homepage": "yarn run switch-homepage && yarn run predeploy && timestamp=$(date -u +\"%FT%TZ\") && gh-pages -b master -d build -m \"Update $timestamp\" && yarn run switch-event",
    "deploy-all": "yarn run deploy-event && yarn run deploy-homepage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "purgecss": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HackTJ/2021.git"
  },
  "bugs": {
    "url": "https://github.com/HackTJ/2021/issues"
  }
}
