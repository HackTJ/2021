{
  "name": "hacktj-2021",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org/2021",
  "dependencies": {
    "chart.js": "^3.0.0-beta.13",
    "dayjs": "^1.10.4",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.1.1",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.9",
    "react-map-gl": "^6.1.10",
    "react-parallax-tilt": "^1.4.91",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "reactstrap": "^8.9.0",
    "web-vitals": "^1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "run switch homepage && react-scripts start",
    "build:js": "react-scripts build",
    "build:css": "purgecss --content build/**/*.{html,js} --css build/**/*.css --output build/static/css --font-face --keyframes --rejected",
    "build": "run build:js && run build:css && react-snap; run build:css",
    "test": "react-scripts test",
    "lint": "prettier . --write; eslint src/ --fix",
    "tint": "node tint-sponsor-images.js",
    "switch": "./switch",
    "deploy": "./deploy"
  },
  "reactSnap": {
    "minifyCss": {
      "inline": [
        "all"
      ],
      "level": 2,
      "sourceMap": true,
      "sourceMapInlineSources": true
    },
    "fixWebpackChunksIssue": false,
    "inlineCss": false,
    "cacheAjaxRequests": true,
    "http2PushManifest": true,
    "preloadImages": true,
    "asyncScriptTags": true
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@types/chart.js": "^2.9.31",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-map-gl": "^5.2.9",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "htmlparser2": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jimp": "^0.16.1",
    "prettier": "^2.2.1",
    "preval.macro": "^5.0.0",
    "purgecss": "^4.0.2",
    "react-scripts": "^4.0.3",
    "react-snap": "^1.23.0",
    "serve": "^11.3.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HackTJ/2021.git"
  },
  "bugs": {
    "url": "https://github.com/HackTJ/2021/issues"
  }
}
